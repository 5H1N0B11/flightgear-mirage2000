<?xml version="1.0" encoding="iso-8859-1"?>

<PropertyList>

  <path>R-74Smoke.ac</path>

  <animation>
    <type>dist-scale</type>
    <object-name>r-74</object-name>
    <interpolation>
      <entry>
        <ind>0</ind>
        <dep>0.3</dep>
      </entry>
      <entry>
        <ind>40</ind>
        <dep>1</dep>
      </entry>
      <entry>
        <ind>16000</ind>
        <dep>4</dep>
      </entry>
    </interpolation>
  </animation>

  <animation>
    <type>material</type>
    <object-name>flame</object-name>
    <emission>
      <red>   255 </red>
      <green> 255 </green>
      <blue>  254 </blue>
    </emission>
  </animation>

  <model>
    <path>Aircraft/Mirage-2000/Missiles/R74/smoke.xml</path>
    <offsets>
      <x-m>3</x-m>
      <y-m>0</y-m>
      <z-m>0</z-m>
    </offsets>
  </model>

</PropertyList>
