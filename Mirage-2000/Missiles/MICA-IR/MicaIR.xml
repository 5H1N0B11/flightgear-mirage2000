<?xml version="1.0" encoding="iso-8859-1"?>
<PropertyList>
  <!-- Source   : http://en.wikipedia.org/wiki/MICA_(missile)
                : http://www.aviationsmilitaires.net/display/missile/126

       Picture  : http://www.ausairpower.net/NATO/Matra-MICA-S.jpg
                : http://www.aviationsmilitaires.net/pictures/views/mica.jpg

       Lenght   : 3.10 m
       Diameter : 0.160 m
       Wingspan : 0.560 m
       Weight   : 112 kg (246.91 lb)
  -->
  <path>Aircraft/Mirage-2000/Missiles/MICA-IR/mica_ir.ac</path>
  <animation>
    <!-- Objets opaques -->
    <object-name>mica_ir</object-name>
    <!-- Objets transparents -->
    <object-name>mica_ir.optique</object-name>
  </animation>
  <effect>
    <inherits-from>Aircraft/Mirage-2000/Models/Effects/Glass/glass</inherits-from>
    <object-name>mica_ir.optique</object-name>
  </effect>
  <animation>
    <enable-hot type="bool">false</enable-hot>
  </animation>
</PropertyList>
