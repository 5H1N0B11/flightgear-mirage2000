<?xml version="1.0" encoding="iso-8859-1"?>

<PropertyList>

  <path>logos.ac</path>

  <animation>
    <object-name>logos</object-name>
    <object-name>logospoilerG</object-name>
    <object-name>logospoilerD</object-name>
    <object-name>logosaileronG</object-name>
    <object-name>logosaileronD</object-name>
    <object-name>logoselevonG</object-name>
    <object-name>logoselevonD</object-name>
    <object-name>logosdirection</object-name>
    <object-name>serial</object-name>
    <object-name>logos-BIPLACE</object-name>
    <object-name>logos-MONOPLACE</object-name>
  </animation>

  <!-- working for Rembrandt -->
  <effect>
    <inherits-from>Effects/model-transparent</inherits-from>
    <object-name>logos</object-name>
    <object-name>logospoilerG</object-name>
    <object-name>logospoilerD</object-name>
    <object-name>logosaileronG</object-name>
    <object-name>logosaileronD</object-name>
    <object-name>logoselevonG</object-name>
    <object-name>logoselevonD</object-name>
    <object-name>logosdirection</object-name>
    <object-name>serial</object-name>
    <object-name>logos-BIPLACE</object-name>
    <object-name>logos-MONOPLACE</object-name>
  </effect>

  <animation>
    <type>material</type>
    <object-name>logos</object-name>
    <object-name>logospoilerG</object-name>
    <object-name>logospoilerD</object-name>
    <object-name>logosaileronG</object-name>
    <object-name>logosaileronD</object-name>
    <object-name>logoselevonG</object-name>
    <object-name>logoselevonD</object-name>
    <object-name>logosdirection</object-name>
    <object-name>serial</object-name>
    <object-name>logos-BIPLACE</object-name>
    <object-name>logos-MONOPLACE</object-name>

    <property-base>sim/model/logos</property-base>
    <texture-prop>texture</texture-prop>
    <texture>default.png</texture>
    <specular>
      <red-prop>emission/red</red-prop>
      <green-prop>emission/green</green-prop>
      <blue-prop>emission/blue</blue-prop>
    </specular>
  </animation>

  <!-- HIDING MONOPLACE PARTS -->
  <animation>
    <type>select</type>
    <object-name>logos-MONOPLACE</object-name>
    <condition>
      <false />
    </condition>
  </animation>

  <!-- Spoilers -->
  <animation>
    <type>rotate</type>
    <object-name>logospoilerG</object-name>
    <property>sim/multiplay/generic/float[22]</property>
    <interpolation>
      <entry><ind> 0 </ind><dep>   0 </dep></entry>
      <entry><ind> 55 </ind><dep> -55 </dep></entry>
    </interpolation>
    <axis>
      <x1-m>  0.913 </x1-m>
      <y1-m> -1.587 </y1-m>
      <z1-m> -0.866 </z1-m>
      <x2-m>  0.913 </x2-m>
      <y2-m> -1.333 </y2-m>
      <z2-m> -0.841 </z2-m>
    </axis>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>logospoilerD</object-name>
    <property>sim/multiplay/generic/float[22]</property>
    <interpolation>
      <entry><ind> 0 </ind><dep>   0 </dep></entry>
      <entry><ind> 55 </ind><dep> -55 </dep></entry>
    </interpolation>
    <axis>
      <x1-m>  0.913 </x1-m>
      <y1-m>  1.333 </y1-m>
      <z1-m> -0.841 </z1-m>
      <x2-m>  0.943 </x2-m>
      <y2-m>  1.587 </y2-m>
      <z2-m> -0.866 </z2-m>
    </axis>
  </animation>

  <!-- Gouverne de direction -->
  <animation>
    <type>rotate</type>
    <object-name>logosdirection</object-name>
    <property>sim/multiplay/generic/float[19]</property>
    <factor> 1 </factor>
    <axis>
      <x1-m>5.895</x1-m>
      <y1-m>0.000</y1-m>
      <z1-m>0.357</z1-m>
      <x2-m>6.439</x2-m>
      <y2-m>0.000</y2-m>
      <z2-m>1.778</z2-m>
    </axis>
  </animation>

  <!-- Animations des ailerons/elevons -->
  <animation>
    <type>rotate</type>
    <object-name>logosaileronG</object-name>
    <property>sim/multiplay/generic/float[15]</property>
    <factor> 1 </factor>
    <axis>
      <x1-m> 4.321</x1-m>
      <y1-m>-4.290</y1-m>
      <z1-m>-1.250</z1-m>
      <x2-m> 4.309</x2-m>
      <y2-m>-2.324</y2-m>
      <z2-m>-1.093</z2-m>
    </axis>
  </animation>

   <animation>
    <type>rotate</type>
    <object-name>logosaileronD</object-name>
    <property>sim/multiplay/generic/float[18]</property>
    <factor> 1 </factor>
    <axis>
      <x1-m>  4.309 </x1-m>
      <y1-m>  2.324 </y1-m>
      <z1-m> -1.093 </z1-m>
      <x2-m>  4.321 </x2-m>
      <y2-m>  4.290 </y2-m>
      <z2-m> -1.250 </z2-m>
    </axis>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>logoselevonG</object-name>
    <property>sim/multiplay/generic/float[16]</property>
    <factor> 1 </factor>
    <axis>
      <x1-m>  4.309 </x1-m>
      <y1-m> -2.322 </y1-m>
      <z1-m> -1.093 </z1-m>
      <x2-m>  4.308 </x2-m>
      <y2-m> -0.873 </y2-m>
      <z2-m> -0.973 </z2-m>
    </axis>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>logoselevonD</object-name>
    <property>sim/multiplay/generic/float[17]</property>
    <factor> 1 </factor>
    <axis>
      <x1-m>  4.308 </x1-m>
      <y1-m>  0.873 </y1-m>
      <z1-m> -0.973 </z1-m>
      <x2-m>  4.309 </x2-m>
      <y2-m>  2.322 </y2-m>
      <z2-m> -1.093 </z2-m>
    </axis>
  </animation>


  <!-- Animation des portes des train -->
  <animation>
    <type>rotate</type>
    <object-name>serial</object-name>
    <property>gear/gear[0]/position-norm</property>
    <interpolation>
      <entry><ind> 0.0 </ind><dep>  0 </dep></entry>
      <entry><ind> 0.6 </ind><dep>  0 </dep></entry>
      <entry><ind> 1.0 </ind><dep> 75 </dep></entry>
    </interpolation>
    <center>
      <x-m> -3.331 </x-m>
      <y-m>  0.000 </y-m>
      <z-m> -1.324 </z-m>
    </center>
    <axis>
      <x> 0 </x>
      <y> 1 </y>
      <z> 0 </z>
    </axis>
  </animation>

</PropertyList>
