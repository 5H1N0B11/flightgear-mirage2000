<?xml version="1.0" encoding="iso-8859-1"?>

<PropertyList>

  <path>exhaust.ac</path>

  <animation>
    <!-- opaques objects  -->
    <object-name>tuyere01</object-name>
    <object-name>tuyere02</object-name>

    <!-- transparents objects  -->

  </animation>

  <!-- ~~~~~~~~~~~~~~~~~~ engine n2 -->
  <animation>
    <type>rotate</type>
    <object-name>tuyere01</object-name>
    <property>engines/engine[0]/nozzle-pos-norm</property>
    <factor>-15</factor>
    <offset-deg>15</offset-deg>
    <axis>
      <x> 0 </x>
      <y> 1 </y>
      <z> 0 </z>
    </axis>
  </animation>

  <!-- ~~~~~~~~~~~~~~~~~~ engine n2 -->
  <animation>
    <type>rotate</type>
    <object-name>tuyere02</object-name>
    <property>engines/engine[0]/nozzle-pos-norm</property>
    <factor>-75</factor>
    <offset-deg>53</offset-deg>
    <center>
      <x-m>  0.00000 </x-m>
      <y-m>  0.00000 </y-m>
      <z-m> -0.05700 </z-m>
    </center>
    <axis>
      <x> 0 </x>
      <y> 1 </y>
      <z> 0 </z>
    </axis>
  </animation>

</PropertyList>
