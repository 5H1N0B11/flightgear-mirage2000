<?xml version="1.0" encoding="iso-8859-1"?>

<PropertyList>

  <path>fuse.ac</path>

  <animation>
    <!-- Objets opaques -->
    <object-name>fuse</object-name>

    <!-- Objets transparents -->
  </animation>

  <!-- Objets Lights -->
  <animation>
    <type>material</type>
    <object-name>fuse</object-name>
    <emission>
      <factor-prop>/systems/electrical/outputs/instrument-lights</factor-prop>
      <red>   0.50 </red>
      <green> 0.25 </green>
      <blue>  0.25 </blue>
    </emission>
  </animation>


</PropertyList>
