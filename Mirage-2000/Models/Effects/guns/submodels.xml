    <count>-1</count>
    <x-offset>-3</x-offset>
    <y-offset> 0</y-offset>
    <z-offset>-8</z-offset>
    <eda>0.1</eda>
    <life>5</life>
    <buoyancy>28</buoyancy>
    <aero-stabilise>false</aero-stabilise>
    <wind>true</wind>
    <pitch-offset>-35.0</pitch-offset>
    <yaw-offset>2</yaw-offset>
  </submodel>

